<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import com.jfoenix.controls.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="470.0" prefWidth="650.0" style="-fx-background-color: #2A3048;" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxtesis.FirstSceneController">
    <stylesheets>
        <URL value="@firstscene.css" />
    </stylesheets>
   <children>
      <AnchorPane prefHeight="470.0" prefWidth="200.0" style="-fx-background-color: #677DD6;">
         <children>
            <Label fx:id="titulo" layoutX="45.0" layoutY="25.0" text="FaceSecurity" textFill="SNOW">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <VBox layoutY="136.0">
               <children>
                  <JFXButton fx:id="ingreso" buttonType="RAISED" onAction="#ingreso" prefHeight="31.0" prefWidth="210.0" ripplerFill="#ab2727" text="Ingresar" textFill="#bbbbbb">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <graphic>
                        <ImageView fitHeight="18.0" fitWidth="17.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@images/icons8_Key_50px_1.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </JFXButton>
               </children>
            </VBox>
         </children>
      </AnchorPane>
      <Label fx:id="cerrar" layoutX="622.0" layoutY="8.0" onMouseClicked="#cerrar" text="X" textFill="WHITE">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <AnchorPane fx:id="datos" layoutX="301.0" layoutY="96.0" prefHeight="279.0" prefWidth="256.0" style="-fx-background-color: #677DD6;" visible="false">
         <children>
            <Label fx:id="sesion" layoutX="69.0" layoutY="14.0" text="Inicio de Sesión" textFill="#fffafa">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <JFXTextField fx:id="userText" focusColor="#bbbbbb" labelFloat="true" layoutX="29.0" layoutY="58.0" prefHeight="31.0" prefWidth="200.0" promptText="Usuario" unFocusColor="#bbbbbb">
               <font>
                  <Font size="15.0" />
               </font>
            </JFXTextField>
            <JFXPasswordField fx:id="passText" focusColor="#bbbbbb" labelFloat="true" layoutX="29.0" layoutY="114.0" prefHeight="31.0" prefWidth="200.0" promptText="Contraseña" unFocusColor="#bbbbbb">
               <font>
                  <Font size="15.0" />
               </font>
            </JFXPasswordField>
            <JFXButton fx:id="entrar" buttonType="RAISED" layoutX="29.0" layoutY="203.0" onAction="#datosAdmin" ripplerFill="#ab2727" text="Inicio" textFill="#bbbbbb">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="94.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/icons8_Checkmark_50px.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
            <JFXButton fx:id="atras" buttonType="RAISED" layoutX="127.0" layoutY="203.0" onAction="#regreso" ripplerFill="#ab2727" text="Cerrar" textFill="#bbbbbb">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="105.0" fitWidth="12.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/icons8_Delete_50px.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
